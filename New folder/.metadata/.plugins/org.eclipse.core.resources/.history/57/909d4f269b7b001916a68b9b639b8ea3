package com.evry.basicExamples;

import java.util.ArrayList;

public class StreamsExampleDemo {

	public static void main(String[] args) {

		ArrayList<Integer> al =new ArrayList<Integer>();
		
		al.add(5);
		al.add(10);
		al.add(15);
		al.add(20);
		al.add(25);
		al.add(30);
		
		System.out.println(al);
		
		
	}

}
